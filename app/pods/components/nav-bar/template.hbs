<ul class="title-area">
  <li class="name">
    <h1><a href="#">airember</a></h1>
  </li>
   <!-- Remove the class "menu-icon" to get rid of menu icon. Take out "Menu" to just have icon alone -->
  <li class="toggle-topbar menu-icon"><a href="#"><span>Menu</span></a></li>
</ul>

<section class="top-bar-section">
  <!-- Right Nav Section -->
  <ul class="right">
    <li class="active"><a href="#">List Your Home</a></li>
  </ul>

  <!-- Left Nav Section -->
  <ul class="left">
    <li class="has-form">
      {{#auto-complete class="row collapse listing-search" placeholder="Find somewhere to stay"
         action='search' content=listings searchPath='name' as |listing|}}

        <!-- Custom template for the typeahead-->
        <div class="price right">{{listing.price}}</div>

        <div class="listing-name">{{listing.name}}</div>

        <div class="city">{{listing.city}}</div>
      {{/auto-complete}}
    </li>
    <li><a href="#">Browse</a></li>
  </ul>
</section>
